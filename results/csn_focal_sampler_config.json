{
    "data_path": "data/raw/US_Accidents_March23.csv",
    "sample_frac": 0.1,
    "epochs": 20,
    "batch_size": 128,
    "lr": 0.001,
    "device": "cuda",
    "seed": 42,
    "split_strategy": "time",
    "region_a_states": [
        "CA",
        "OR",
        "WA",
        "NV",
        "AZ"
    ],
    "region_b_states": [
        "NY",
        "PA",
        "OH",
        "MI",
        "IL"
    ],
    "model_type": "crash_severity_net",
    "loss_type": "focal",
    "use_sampler": true,
    "use_smote": false,
    "gamma": 2.0,
    "exp_id": "csn_focal_sampler"
}